{
  "parser": "babel-eslint",
  "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "commonjs": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "indent": [2, 2],
    "import/prefer-default-export": "off"
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "classes": true
    }
  }
}
